{
  "expo": {
    "name": "Clinidata",
    "slug": "clinidata",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clinidata.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.clinidata.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/icon3.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker"
    ],
    "scheme": "clinidata",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d3114a9-9071-47ed-9796-5a9776178ac6"
      }
    }
  }
}
